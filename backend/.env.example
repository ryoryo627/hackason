# GCP Configuration
GOOGLE_CLOUD_PROJECT=homecare-ai-dev
GCP_REGION=asia-northeast1

# Firestore
FIRESTORE_DATABASE_ID=(default)

# Cloud Storage
GCS_BUCKET_NAME=homecare-ai-files
GCS_KNOWLEDGE_BUCKET=homecare-ai-knowledge

# Vertex AI
VERTEX_AI_REGION=asia-northeast1
EMBEDDING_MODEL=text-embedding-005

# Gemini
GEMINI_MODEL=gemini-3.0-flash

# Admin UI URL (for CORS)
ADMIN_UI_URL=http://localhost:3000

# Server
HOST=0.0.0.0
PORT=8080
DEBUG=true

# Secret Manager references (these are the secret IDs, not the actual values)
SLACK_BOT_TOKEN_SECRET=slack-bot-token
SLACK_SIGNING_SECRET_SECRET=slack-signing-secret
GEMINI_API_KEY_SECRET=gemini-api-key
